{
  "name": "react-unitable",
  "version": "1.0.0",
  "repository": "https://github.com/krystian-mw/react-unitable.git",
  "author": "krystian-mw <krystian@mwebdev.co.uk>",
  "license": "MIT",
  "scripts": {
    "start:server": "npx nodemon app -w app.js",
    "start:client": "NODE_ENV=development tailwind build ./utils/tailwind.css -c ./tailwind.config.js -o ./src/tailwind.css && react-scripts start",
    "build": "NODE_ENV=production tailwind build ./utils/tailwind.css -c ./tailwind.config.js -o ./src/tailwind.css && react-scripts build"
  },
  "dependencies": {
    "array-move": "^3.0.1",
    "axios": "^0.20.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.4",
    "parcel": "^2.0.0-beta.1",
    "pg": "^8.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.4.3",
    "react-icons": "^3.11.0",
    "react-scripts": "^3.4.3",
    "react-sortable-hoc": "^1.11.0",
    "react-virtualized": "^9.22.2",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "tailwindcss": "^1.8.13"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
